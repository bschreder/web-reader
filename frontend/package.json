{
  "name": "web-reader-frontend",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:clean": "del-cli ~/frontend/dist && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --project unit",
    "test:browser": "vitest run --project browser",
    "test:clean": "del-cli ~/frontend/dist && npm run test:coverage",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@tanstack/react-router": "^1.136.6",
    "@tanstack/react-router-devtools": "^1.136.6",
    "@tanstack/react-start": "^1.136.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/router-cli": "^1.136.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.9",
    "@vitest/browser-preview": "^4.0.9",
    "@vitest/coverage-v8": "^4.0.9",
    "autoprefixer": "^10.4.22",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9",
    "vitest-browser-react": "2.0.2"
  }
}
